<%= form_for( @memorabilia ) do |f| %>
  <% f.hidden_field :athlete_id , :value => @athlete.id %>
  <%= f.label :item_type %>
  <%= f.text_field :item_type %>
  <br>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <br>
  <%= f.label :autographed %>
  <%= f.check_box :autographed %>
  <br>
  
  <%= f.submit %>
<% end %>